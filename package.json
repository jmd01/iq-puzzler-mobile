{
  "name": "iq-puzzler-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:dev-client": "expo start --dev-client",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:android": "eas submit -p android",
    "update:preview": "APP_VARIANT='preview' eas update --branch preview",
    "update:production": "eas update --branch production",
    "lint": "yarn eslint --max-warnings 0 --ext ts,tsx ./src",
    "check:tsc": "yarn tsc --noEmit"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "dotenv": "^16.3.1",
    "expo": "~49.0.11",
    "expo-av": "~13.4.1",
    "expo-dev-client": "~2.4.10",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.15",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.2.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/styled-components-react-native": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.5.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
